<?xml version="1.0" encoding="UTF-8"?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms" 
          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
          x:Class="CoordinatorExampleApp.Views.ViewCells.SideMenuHeaderViewCell">

    <StackLayout Padding="0,0,0,0" BackgroundColor="{StaticResource sideMenuHeaderBackgroundColor}" Margin="0,0,0,0">
        
        <Label Text="{Binding Title}" 
               TextColor="{StaticResource sideMenuHeaderTextColor}" 
               FontSize="14"
               Margin="20,25,0,0"/>
        
    </StackLayout>

</ViewCell>
